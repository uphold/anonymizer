{
  "name": "@uphold/anonymizer",
  "version": "4.2.1",
  "description": "Object redaction with whitelist as main feature.",
  "homepage": "https://github.com/uphold/anonymizer#readme",
  "bugs": {
    "url": "https://github.com/uphold/anonymizer/issues"
  },
  "repository": "uphold/anonymizer",
  "license": "MIT",
  "main": "src",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "changelog": "uphold-scripts changelog $npm_package_version",
    "lint": "eslint --cache . ",
    "release": "uphold-scripts release",
    "test": "TZ=UTC jest",
    "version": "uphold-scripts version"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "modulePaths": ["<rootDir>"],
    "restoreMocks": true,
    "testRegex": "test/.*\\.test.js$"
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "serialize-error": "8.0.0",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "eslint": "~8.20.0",
    "eslint-config-uphold": "^4.1.0",
    "jest": "28.1.3",
    "prettier": "^2.8.3",
    "uphold-scripts": "0.8.0"
  },
  "engines": {
    "node": ">=18"
  },
  "pre-commit": [
    "lint"
  ]
}
